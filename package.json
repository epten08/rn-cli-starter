{
  "name": "ReactNativeStarter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "clean": "cd android && gradlew clean",
    "clean:android": "cd android && gradlew clean",
    "clean:cache": "del /s /q node_modules && npm install",
    "pod:install": "cd ios && bundle install && bundle exec pod install",
    "build:android:release": "cd android && gradlew assembleRelease",
    "build:ios:release": "cd ios && bundle exec fastlane build_release"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --ignore-pattern '*.config.js' --ignore-pattern '.*.js'",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@d11/react-native-fast-image": "^8.13.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native/new-app-screen": "0.83.1",
    "@react-navigation/bottom-tabs": "^7.9.1",
    "@react-navigation/elements": "^2.9.4",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.1",
    "@react-navigation/stack": "^7.6.13",
    "@reduxjs/toolkit": "^2.11.2",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "react": "19.2.3",
    "react-i18next": "^16.5.2",
    "react-native": "0.83.1",
    "react-native-config": "^1.6.1",
    "react-native-device-info": "^15.0.1",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-localize": "^3.6.1",
    "react-native-mmkv": "^4.1.1",
    "react-native-nitro-modules": "^0.33.0",
    "react-native-reanimated": "^4.2.1",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.19.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-worklets": "^0.7.1",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.1",
    "@react-native/eslint-config": "0.83.1",
    "@react-native/metro-config": "0.83.1",
    "@react-native/typescript-config": "0.83.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.2.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-redux": "^7.1.34",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "react-test-renderer": "^19.2.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
